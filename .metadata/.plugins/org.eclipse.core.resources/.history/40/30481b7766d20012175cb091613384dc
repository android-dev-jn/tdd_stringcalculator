package Calculator;

public class Calculator {
	
	int[] numbers = new int[1000];

	public static int add() {
		return 0;
	}

	public static int add(String string) {
		int result = 0;
		if (string.equals("")) {
			// do nothing
		} else {
			numbers = Integer.parseInt(string.split(","));
		}
		return result;
	}

}
